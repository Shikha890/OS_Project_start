#include<stdio.h>
int main()
{
int n;
printf("enter the no of processes: ");
scanf("%d",&n);
printf("\n");
int at[n];
int bt[n];
for(int i=0 ; i<n ; i++)
  {
  	printf("enter the arrival time of PROCESS %d:-> ",i+1);
  	scanf("%d",&at[i]);
  	
  	printf("enter the burst time of PROCESS %d:-> ",i+1);
  	scanf("%d",&bt[i]);
  	
  	printf("\n\n");
  }
  
}
